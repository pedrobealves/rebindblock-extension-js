{
  "name": "RebindBlock",
  "description": "DNS Rebind block.",
  "version": "2.0.0.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage","search", "activeTab", "scripting", "tabs", "webRequest", "declarativeNetRequest"],
  "action": {
    "default_popup": "web/popup.html",
    "default_icon": {
      "16": "/images/icon16.png",
      "32": "/images/icon32.png",
      "48": "/images/icon48.png",
      "64": "/images/icon64.png",
      "128": "/images/icon128.png",
      "256": "/images/icon256.png",
      "512": "/images/icon512.png"
    }
  },
  "icons": {
    "16": "/images/icon16.png",
    "32": "/images/icon32.png",
    "48": "/images/icon48.png",
    "64": "/images/icon64.png",
    "128": "/images/icon128.png",
    "256": "/images/icon256.png",
    "512": "/images/icon512.png"
  },
  "host_permissions": [
    "<all_urls>"
  ]
}
